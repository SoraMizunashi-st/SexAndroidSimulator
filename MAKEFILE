# -----------------------------------------------------------------------------------------------------------------------------
#
# All Object
#
# -----------------------------------------------------------------------------------------------------------------------------
OBJ = $(MAIN_OBJ) $(CORE_OBJ) $(TYPE_OBJ)

# -----------------------------------------------------------------------------------------------------------------------------
#
# Rule
#
# -----------------------------------------------------------------------------------------------------------------------------
.PHONY: all
all: $(TARGET)

$(TARGET): $(OBJ)
	$(CC) $(OPTIMIZE) -o $(TARGET) $(OBJ)

# -----------------------------------------------------------------------------------------------------------------------------
#
# App Object
#
# -----------------------------------------------------------------------------------------------------------------------------
$(MAIN_OBJ): $(MAIN_SRC) $(MAIN_HDR)
	$(CC) $(OPTIMIZE) -c $(MAIN_SRC) -o $(MAIN_OBJ)

# -----------------------------------------------------------------------------------------------------------------------------
#
# Core Object
#
# -----------------------------------------------------------------------------------------------------------------------------
$(CORE_OBJ): $(CORE_SRC) $(CORE_HDR)
	$(CC) $(OPTIMIZE) -c $(CORE_SRC) -o $(CORE_OBJ)

